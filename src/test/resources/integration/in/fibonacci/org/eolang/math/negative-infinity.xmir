   <listing># SPDX-FileCopyrightText: Copyright (c) 2021-2025 Max Trunnikov
# SPDX-License-Identifier: MIT

+alias org.eolang.math.nan
+alias org.eolang.math.positive-infinity
+home https://github.com/objectionary/eo-math
+package org.eolang.math
+rt jvm org.eolang:eo-math:0.2.3
+version 0.2.3

# Negative infinity
[] &gt; negative-infinity
  # Tests that $ = x
  [x] &gt; eq
    eq. &gt; @
      ^.as-bytes
      x.as-bytes

  # Tests that $ ≠ x
  [x] &gt; neq
    (^.eq x).not &gt; @

  # Tests that $ &lt; x
  [x] &gt; lt
    not. &gt; @
      or.
        x.as-bytes.eq (nan.as-bytes)
        ^.eq x

  # Tests that $ ≤ x
  [x] &gt; lte
    not. &gt; @
      eq.
        x.as-bytes
        nan.as-bytes

  # Tests that $ &gt; x
  [x] &gt; gt
    FALSE &gt; @

  # Tests that $ ≥ x
  [x] &gt; gte
    ^.eq x &gt; @

  # Multiplication of $ and x
  [x] &gt; times
    if. &gt; @
      or.
        x.as-bytes.eq (nan.as-bytes)
        x.eq 0.0
      nan
      if.
        x.eq ^
        positive-infinity
        ^

  # Sum of $ and x
  [x] &gt; plus
    if. &gt; @
      or.
        x.as-bytes.eq (nan.as-bytes)
        x.eq positive-infinity
      nan
      ^

  # Negation of $
  [] &gt; neg
    positive-infinity &gt; @

  # Sign of $ (-1.0, 0.0, or 1.0)
  [] &gt; signum
    -1.0 &gt; @

  # Difference between $ and x
  [x] &gt; minus
    ^.plus (x.neg) &gt; @

  # x'th power of $
  [x] &gt; pow
    if. &gt; @
      x.lt 0.0
      0.0
      if.
        x.eq 0.0
        1
        if.
          x.gt 0.0
          if.
            not-int x
            ^
            if.
              (x.as-int.mod 2).eq 0
              positive-infinity
              ^
          nan

    [x] &gt; not-int
      x.as-int &gt; as-integer!
      and. &gt; @
        as-integer.as-float.lt x
        (as-integer.plus 1).as-float.gt x

  # Quotient of the division of $ by x
  [x] &gt; div
    if. &gt; @
      or.
        x.as-bytes.eq (nan.as-bytes)
        x.eq ^
        x.eq positive-infinity
      nan
      ^

  # Remainder of $
  [] &gt; mod
    nan &gt; @

  # Absolute value of $ (i.e., with no sign)
  [] &gt; abs
    positive-infinity &gt; @

  # Converts this to bytes
  [] &gt; as-bytes
    FF-80-00-00 &gt; @

  # Converts this to int
  [] &gt; as-int
    [] &gt; @
      "You can not convert negative infinity to int" &gt; msg

  # Converts this to string
  [] &gt; as-string
    "-∞" &gt; @
</listing>

<program ms="4"
          name="org.eolang.math.number"
          source="/home/olesya/huawei/fork-eo/eo/eo-maven-plugin/target/it/fibonacci/target/eo/04-pull/org/eolang/math/number.eo"
          time="2022-09-09T13:01:02.604996756Z"
